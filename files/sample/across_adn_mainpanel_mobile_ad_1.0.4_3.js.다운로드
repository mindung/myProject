function fn_closing_mobile_pinad() {
	jQuery("#adn_mobliel_pinad_bg_wrap").fadeOut(0);
	if(_adn_mobile_pinad_effect==0) jQuery("#adn_mobliel_pinad_wrap").fadeOut(0);
	else  jQuery("#adn_mobliel_pinad_wrap").fadeOut('slow');
  	jQuery("#adn_mobliel_pinad_wrap").css({"display":"none"});                	

	if (_adn_mobile_pinad_hours != "0") {
		fn_adn_mobile_pinad_setCookie("adnMobilePinAd", "1", _adn_mobile_pinad_hours)
	}
}

function js_adn_mobile_pinad_today() {
    jQuery("#adn_mobliel_pinad_wrap").hide();
    if (document.getElementById('pinad_today').checked) {
        fn_adn_mobile_pinad_setCookie("adnMobilePinAd", "9", 24 - parseInt(now_hours))
    } else {
         if (_adn_mobile_pinad_hours != "0") {
            fn_adn_mobile_pinad_setCookie("adnMobilePinAd", "1", _adn_mobile_pinad_hours)
         }
    }
}

function fn_adn_mobile_pinad_getCookie(a) {
    var b = a + "=";
    var x = 0;
    while (x <= document.cookie.length) {
        var y = (x + b.length);
        if (document.cookie.substring(x, y) == b) {
            if ((endOfCookie = document.cookie.indexOf(";", y)) == -1) {
                endOfCookie = document.cookie.length
            }
            return unescape(document.cookie.substring(y, endOfCookie))
        }
        x = document.cookie.indexOf(" ", x) + 1;
        if (x == 0) {
            break
        }
    }
    return ""
}

function fn_adn_mobile_pinad_setCookie(a, b, c) {
    var d = new Date();
    d.setTime(d.getTime() + (1000 * 60 * 60 * c));
    document.cookie = a + "=" + b + ";path=/;expires=" + d.toGMTString() + ";"
}


function adn_mobile_pinad_frm_send(c, d) {
    var a = document.createElement("iframe");
    a.frameBorder = "0";
    a.scrolling = "no";
    a.width = "0px";
    a.height = "0px";
    a.setAttribute("src", d);
    document.getElementById(c).appendChild(a)
}


function adn_mobile_pinad_objectValues(obj) {
    var res = [];
    for (var i in obj) {
        if (obj.hasOwnProperty(i)) {
            res.push(obj[i])
        }
    }
    return res
}



function adn_mobile_pinad_carousel() {
  var i;
  var x = document.getElementsByClassName("adn_pinad_title_Slides");
  for (i = 0; i < x.length; i++) {
    x[i].style.display = "none"; 
  }
  _adn_mobile_pinad_title_slideIndex++;
  if (_adn_mobile_pinad_title_slideIndex > x.length) {_adn_mobile_pinad_title_slideIndex = 1} 
  x[_adn_mobile_pinad_title_slideIndex-1].style.display = "block"; 
  setTimeout(adn_mobile_pinad_carousel, 1500); 
}
		

