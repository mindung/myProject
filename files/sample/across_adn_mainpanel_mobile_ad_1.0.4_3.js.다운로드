																         feed_slide_status =  jQuery(".pinad_feed_Slider").slick({
																              dots: true,
																              autoplay: true,
																              autoplaySpeed: 3000,
																              infinite: true,
																              slidesToShow: 1,
																              slideswToScroll: 1,
																              arrows: false,
																              draggable : false,
																          });
																          try{
																		     jQuery('.pinad_feed_Slider').on('afterChange', function(event, slick, currentSlide, nextSlide){
																		         // no dots -> no slides
																				  if(!slick.$dots){
																				    return;
																				  }
																				
																				  //currentSlide is undefined on init -- set it to 0 in this case (currentSlide is 0 based)
																				  var i = (currentSlide ? currentSlide : 0) + 1;
																				  // use dots to get some count information
																				  $status.text(i + '/' + (slick.$dots[0].children.length));
																		     });
																		    }catch(e){
																				jQuery( ".pinad_feed_Slider" ).bind('afterChange', function(event, slick, currentSlide, nextSlide){
																		         // no dots -> no slides
																				  if(!slick.$dots){
																				    return;
																				  }
																				
																				  //currentSlide is undefined on init -- set it to 0 in this case (currentSlide is 0 based)
																				  var i = (currentSlide ? currentSlide : 0) + 1;
																				  // use dots to get some count information
																				  $status.text(i + '/' + (slick.$dots[0].children.length));
																		     });
																		    }
																		}
																		else
																		{
																         feed_slide_status =  jQuery(".pinad_feed_Slider").slick({
																              dots: true,
																              autoplay: true,
																              autoplaySpeed: 3000,
																              infinite: true,
																              slidesToShow: 1,
																              slideswToScroll: 1,
																              arrows: false,
																              draggable : false,
																          });

																		}
																	}
	

														      imp_url = "https://kn.acrosspf.com/log/pinad_imp.ad?ui=" + ui + "&ci=" + ci + "&gi=" + gi + "&rnd=" + Math.floor(Math.random() * 99999999999);
														      var imp_id = "adn_pinad_imp_" + Math.floor(Math.random() * 99999999999);
														      var a = document.createElement("DIV");
														      a.setAttribute("id", imp_id);
														      a.setAttribute("style", "display:none;width:0px;height:0px;");
														      document.getElementById("adn_mobliel_pinad_wrap").appendChild(a);
														      adn_mobile_pinad_frm_send(imp_id, imp_url);
														}
						           		}
							          },
							          error: function(xhr) {
							              jQuery("#adn_mobliel_pinad_wrap").hide();
							              is_mobile_pinad_status = false
							          }
							      })
							  } catch (e) {}



    
					}
			}	
		});
	});
	
});